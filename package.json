{
  "name": "drocket",
  "type": "module",
  "version": "1.0.24",
  "files": [
    "dist"
  ],
  "private": false,
  "module": "./dist/drocket.es.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/drocket.es.js",
      "require": "./dist/drocket.umd.js"
    },
    "./framework.scss": {
      "import": "./dist/styles/main.scss",
      "require": "./dist/styles/main.scss"
    },
    "./styles.css": {
      "import": "./dist/index.css",
      "require": "./dist/index.css"
    },
    "./setting.scss": {
      "import": "./dist/styles/override/variables/index.scss",
      "require": "./dist/styles/override/variables/index.scss"
    },
    "./mixin.scss": {
      "import": "./dist/styles/_mixins.scss",
      "require": "./dist/styles/_mixins.scss"
    }
  },
  "types": "./dist/types/index.d.ts",
  "main": "./dist/drocket.umd.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/everleandro/drocket.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "vue",
    "vue3",
    "component",
    "material",
    "library",
    "ui framework",
    "vue framework",
    "ui library",
    "component library",
    "material components",
    "typescript"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly && move ./dist/src ./dist/types",
    "build-linux": "vite build && vue-tsc --emitDeclarationOnly && mv ./dist/src ./dist/types",
    "preserve": "vite build",
    "serve": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "exit 0"
  },
  "peerDependency": {
    "vue": "^3.3.11"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@vitejs/plugin-vue": "^4.6.2",
    "sass": "^1.70.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.12",
    "vite-plugin-css-injected-by-js": "^3.3.1",
    "vite-plugin-dts": "^3.7.1",
    "vue-router": "^4.2.5",
    "vue-tsc": "^1.8.27"
  }
}
